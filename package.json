{
  "name": "ebanet-printer-client",
  "version": "1.0.0",
  "description": "Cliente para impresiones desde EBANET",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "debug": "electron . --remote-debugging-port=9222",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make32": "electron-forge make --arch=ia32"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "^12.0.6"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.17.1",
    "multer": "^1.4.2",
    "pdf-to-printer": "^2.0.5"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./icon.ico",
        "win32metadata":{
          "ProductName": "EBANET - Cliente de impresión",
          "CompanyName": "Fibo Systems"
        }
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "ebanet-printer-client",
            "title": "EBANET - Cliente de impresión",
            "owners": "Fibo Systems"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
